{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "graphql"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "discountcodes",
      "collectionName": "discountcodes",
      "connectionStringSetting": "discountcodedb_DOCUMENTDB",
      "partitionKey": "/id"
    }
  ],
  "scriptFile": "../dist/index.js"
}
